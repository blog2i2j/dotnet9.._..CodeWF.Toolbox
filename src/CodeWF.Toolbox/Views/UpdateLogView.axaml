<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:md="clr-namespace:MarkdownAIRender.Controls.MarkdownRender;assembly=MarkdownAIRender"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:vm="clr-namespace:CodeWF.Toolbox.ViewModels"
             x:DataType="vm:UpdateLogViewModel" Padding="20"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CodeWF.Toolbox.Views.UpdateLogView">
    <ScrollViewer>
        <md:MarkdownRender Value="{Binding UpdateLogMarkdownContent}" />
    </ScrollViewer>
</UserControl>