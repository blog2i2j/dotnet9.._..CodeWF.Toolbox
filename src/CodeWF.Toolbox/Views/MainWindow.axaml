<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="CodeWF.Toolbox.Views.MainWindow"
        xmlns:views="clr-namespace:CodeWF.Toolbox.Views"
        xmlns:localizer="clr-namespace:CodeWF.Toolbox.Localizer"
        xmlns:i18nKeys="clr-namespace:CodeWF.Toolbox.Assets.i18n"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Width="1000" Height="600"
        d:DesignWidth="1000" d:DesignHeight="600"
        TransparencyLevelHint="AcrylicBlur"
        ExtendClientAreaToDecorationsHint="True"
        Icon="/Assets/logo.ico" CornerRadius="30"
        Title="{localizer:Localize {x:Static i18nKeys:CultureNames.AppName}}">
    <views:MainView />
</Window>